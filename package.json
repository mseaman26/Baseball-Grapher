{
  "name": "baseball-grapher",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js --ignore client",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "scrape": "node server/webScraping/currentSeaonScrape.js"
  },
  "engines": {
    "node": "16.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mseaman26/Baseball-Grapher.git"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mseaman26/Baseball-Grapher/issues"
  },
  "homepage": "https://github.com/mseaman26/Baseball-Grapher#readme",
  "dependencies": {
    "chartjs-plugin-zoom": "^2.0.1",
    "cheerio": "^1.0.0-rc.12"
  }
}
